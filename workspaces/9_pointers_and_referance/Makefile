.PHONY: clean All

All:
	@echo "----------Building project:[ 2_dereferencing_pointers - Debug ]----------"
	@cd "2_dereferencing_pointers" && "$(MAKE)" -f  "2_dereferencing_pointers.mk"
clean:
	@echo "----------Cleaning project:[ 2_dereferencing_pointers - Debug ]----------"
	@cd "2_dereferencing_pointers" && "$(MAKE)" -f  "2_dereferencing_pointers.mk" clean
